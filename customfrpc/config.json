{
  "name": "Custom FRPC",
  "version": "0.0.1",
  "slug": "customfrpc",
  "description": "customfrpc",
  "url": "https://github.com/chongkk/custom-addons/tree/main/customfrpc",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "map": ["ssl:rw"],
  "options": {
    "lets_encrypt": {
      "accept_terms": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "token": null,
    "domains": [null],
    "duckdnsdomain": [null],
    "aliases": [],
    "seconds": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str"
    },
    "ipv4": "str?",
    "ipv6": "str?",
    "token": "str",
    "domains": ["str"],
    "duckdnsdomain": ["str"],
    "aliases": [
      {"domain": "str", "alias": "str"}
    ],
    "seconds": "int"
  },
  "image": "chongxiii/{arch}-addon-customfrpc"
}
